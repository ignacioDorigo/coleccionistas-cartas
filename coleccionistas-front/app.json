{
  "expo": {
    "name": "coleccionistas-front",
    "slug": "coleccionistas-front",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#240046"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
    [
      "expo-image-picker",
      {
        "photosPermission": "Findex necesita acceder a tu galería para seleccionar imágenes."
      }
    ]
  ]
  }
}
